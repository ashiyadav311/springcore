            <list>
                <value>156456</value>
                <value>987456</value>
                <value>743456</value>
            </list>
        </property>
        <property name="Address">
            <props>
                <prop key="Street">Hanuman Road</prop>
                <prop key="City">Lucknow</prop>
                <prop key="Country">India</prop>
            </props>
        </property>
    </bean>
    <bean class="com.springcore.collection.Employee" name="Emp4">
        <constructor-arg value="Virat Kohli"/>
        <constructor-arg value="28"/>
        <constructor-arg>
            <list>
                <value>156456</value>
                <value>987456</value>
                <value>743456</value>
            </list>
        </constructor-arg>
        <constructor-arg>
            <props>
                <prop key="Street">veer bahadur marg</prop>
                <prop key="City">Delhi</prop>
                <prop key="Country">India</prop>
            </props>
        </constructor-arg>
    </bean>
    <bean class="com.springcore.collection.Employee" name="Emp5" p:Name="Nisha Sindhia" p:Age="25">
        <property name="Number">
            <list>
                <value>156456</value>
                <value>987456</value>
                <value>743456</value>
            </list>
        </property>
        <property name="Address">
            <props>
                <prop key="Street">kalkaji</prop>
                <prop key="City">Faizabad</prop>
                <prop key="Country">India</prop>
            </props>
        </property>
    </bean>
    <bean class="com.springcore.collection.Employee" name="Emp6" c:Name="Nisha Sindhia" c:Age="25">
        <property name="Number">
            <list>
                <value>156456</value>
                <value>987456</value>
                <value>743456</value>
            </list>
        </property>
        <property name="Address">
            <props>
                <prop key="Street">kalkaji</prop>
                <prop key="City">Faizabad</prop>
                <prop key="Country">India</prop>
            </props>
        </property>
